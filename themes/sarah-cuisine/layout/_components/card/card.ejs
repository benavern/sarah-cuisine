<%
var types = ['text', 'button', 'image', 'image-link', 'html']

function getType (item) {
  if (item.type && types.includes(item.type)) return item.type
  return 'text'
}
%>

<div class="card">
  <% items.filter(item => !item.hide).forEach(item => { %>
    <%- partial(`./item/${getType(item)}`, { item }) %>
  <% }) %>
</div>
